<h1>Exercise List</h1>

{% if exercises is not empty %}
    <ul>
        {% for exercise in exercises %}
            <li>
                <a href="/exercise/{{ exercise.id }}"> {{ exercise.name }}</a> </li>
        {% endfor %}
    </ul>
{% else %}
    <p>No exercises found.</p>
{% endif %}
<button onclick="history.back()">Back</button>
